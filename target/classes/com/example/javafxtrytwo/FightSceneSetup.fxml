<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<!-- the last part of the below line "fx:controller" indicates the controller/scope -->

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxtrytwo.FightSceneSetupController">
   <children>
      <Button fx:id="button_goto_fight_scene" layoutX="662.0" layoutY="360.0" mnemonicParsing="false" onAction="#openFightScene" text="Go to FightScene">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Text layoutX="394.0" layoutY="68.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Fight Scene Setup.">
         <font>
            <Font size="25.0" />
         </font>
      </Text>
      <Button fx:id="button_inspect_heroes" layoutX="133.0" layoutY="283.0" mnemonicParsing="false" onAction="#openHeroSheetScene" text="Select Hero">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button fx:id="button_menu" layoutX="43.0" layoutY="39.0" mnemonicParsing="false" onAction="#openMainMenu" text="Back to Menu">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="button_set_map" layoutX="347.0" layoutY="430.0" mnemonicParsing="false" onAction="#mapButtonClick" text="Select Background">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button fx:id="button_set_difficulty" layoutX="127.0" layoutY="427.0" mnemonicParsing="false" onAction="#difficultyButtonClick" text="Select Difficulty">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Text layoutX="127.0" layoutY="409.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Difficulty:">
         <font>
            <Font size="25.0" />
         </font>
      </Text>
      <Text layoutX="347.0" layoutY="412.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Background:">
         <font>
            <Font size="25.0" />
         </font>
      </Text>
      <Text layoutX="133.0" layoutY="256.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Hero:">
         <font>
            <Font size="25.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
